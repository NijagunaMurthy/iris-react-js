{
  "name": "iris-react-sdk",
  "version": "1.0.13",
  "description": "React version of Iris JS SDK",
  "license": "MIT",
  "main": "dist/index.js",
  "author": "Comcast",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/iris-platform/iris-react-js"
  },
  "keywords": [
    "iris",
    "react"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ]
  },
  "dependencies": {
    "iris-js-sdk": "^3.3.11",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "react-scripts": "1.0.10",
    "babel-cli": "6.10.1",
    "babel-jest": "14.1.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.11.0",
    "jest": "14.1.0",
    "jquery": "3.1.0",
    "json-loader": "0.5.4",
    "npm-run-all": "2.3.0",
    "react-test-renderer": "15.3.0",
    "rimraf": "2.5.3",
    "webpack": "1.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build1": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename iris.react.js --config webpack.config.js",
    "build:umd.min": "webpack --output-filename iris.react.min.js -p --config webpack.config.js"
  }
}
